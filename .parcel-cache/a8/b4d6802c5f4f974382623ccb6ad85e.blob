@import url("https://fonts.googleapis.com/css?family=Maven+Pro:400,500");
.app-container {
  background-image: #dbdcff;
  background-image: radial-gradient(at 0% 0%, #ebfaf7 0, transparent 44%), radial-gradient(at 51% 100%, #c3d0fe 0, transparent 50%), radial-gradient(at 100% 100%, #e6edff 0, transparent 31%), radial-gradient(at 1% 99%, #c5cdf7 0, transparent 53%), radial-gradient(at 100% 0%, #e6ecff 0, transparent 23%), radial-gradient(at 34% 0%, #aad9ee 0, transparent 43%), radial-gradient(at 100% 48%, #d7e5fe 0, transparent 100%);
  background-size: cover;
  background-repeat: no-repeat;
}
.page-container {
  margin: 4em;
}
.page-container#home {
  text-align: center;
}
.page-container:not(#home) {
  display: none;
}
html {
  font-size: 100%;
}
/*16px*/
body {
  background-color: rgba(255, 255, 255, 0.6);
  font-family: "Maven Pro", sans-serif;
  font-weight: 400;
  line-height: 1.75;
  color: #000000;
}
p {
  margin-bottom: 1rem;
}
h1, h2, h3, h4, h5 {
  font-family: "Maven Pro", sans-serif;
  font-weight: 500;
  line-height: 1.3;
}
h1 {
  margin-top: 0;
  font-size: 40px;
}
h2 {
  font-size: 2.441rem;
}
h3 {
  font-size: 1.953rem;
}
h4 {
  font-size: 1.563rem;
}
h5 {
  font-size: 1.25rem;
}
small, .text_small {
  font-size: 0.8rem;
}
nav {
  width: 100%;
  background-color: #123;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
nav .nav-links {
  list-style: none;
  display: flex;
}
nav .nav-links .nav-item a {
  display: inline-block;
  text-decoration: none;
  color: white;
  padding: 1em 2em;
}
nav .nav-links .nav-item a:hover {
  color: #abc;
}
nav .nav-links .nav-item a:active {
  font-weight: bolder;
}
#task-grid {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 1fr;
  grid-template-areas: "taskform tasklist";
}
#taskform {
  grid-area: taskform;
  max-width: 300px;
  margin: 0 auto;
}
#taskform label {
  display: block;
  width: 100%;
}
#tasklist {
  grid-area: tasklist;
}
