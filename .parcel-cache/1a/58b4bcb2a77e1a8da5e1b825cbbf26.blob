<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
	<title>studBud</title>
	<link href="ac2b17b88ef0ab3e3e3316d5a6be3892" rel="stylesheet" type="text/css">
</head>

<body class="app-container">
	
	<div class="grid-container">
		<div class="header">
		  <div class="header-item">
			<h1>StudBud</h1>
		  </div>
		  <div class="header-item">
			<button class="dark-blue-btn">Instruction</button>
		  </div>
		</div>

		<div class="features">
		  <div class="timer"></div>
		  <div class="music"></div>
		  <div class="dictionary"></div>
		</div>

		<div class="task">
			<div class="task-header">
			<div class="header-item">
				<h1>Today's Task List</h1>
			</div>
			<div class="header-item">
				<button class="light-blue-btn"><i class="fas fa-plus"></i>Add Tasks</button>
			</div>
			</div>
			<div class="quadrants">
			<div class="urgent">
				<h1>Urgent</h1>
			</div>
			<div class="not-urgent">
				<h1>Not Urgent</h1>
			</div>
			<div class="important">
				<h1>Important</h1>
			</div>
			<div class="not-important">
				<h1>Not Important</h1>
			</div>
			<div class="cards">
				<div class="do-it-card"></div>
				<div class="schedule-card"></div>
				<div class="relegate-card"></div>
				<div class="dont-do-card"></div>
			</div>
			</div>
		</div>
	</div>
	
<script src="4e6340146f7d0152c44dbd6ccf58b0e0"></script>
<!-- Font Awesome Icon -->
<script src="925725076c37bf67dc6b65763f8482e9" crossorigin="anonymous"></script>
</body>

</html>