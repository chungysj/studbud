  margin-bottom: 25px;
}
.quadrants {
  display: grid;
  grid-template-columns: -webkit-max-content;
  grid-template-columns: max-content;
  grid-template-rows: 100px minmax(30px, 450px) minmax(30px, 300px);
  gap: 0px 0px;
  grid-template-areas: "space urgent not-urgent" "important cards cards" "not-important cards cards";
  grid-area: quadrants;
  background-color: #ffff;
  border-radius: 30px;
  padding-bottom: 50px;
}
.space {
  grid-area: space;
}
.urgent, .not-urgent, .important, .not-important {
  text-align: center;
}
.urgent {
  grid-area: urgent;
}
.not-urgent {
  grid-area: not-urgent;
}
.important {
  grid-area: important;
}
.not-important {
  grid-area: not-important;
}
.urgent, .not-urgent {
  padding: 30px 0px 15px;
}
.important, .not-important {
  -ms-writing-mode: tb-rl;
      writing-mode: vertical-rl;
  transform: rotate(-180deg);
  padding: 15px;
  margin-top: 50px;
}
.cards {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 1fr 1fr;
  gap: 35px 35px;
  grid-template-areas: "do-it-card schedule-card" "relegate-card dont-do-card";
  grid-area: cards;
  padding-right: 50px;
}
.cards .task-cards {
  border-radius: 30px;
  padding: 40px;
  overflow-y: auto;
}
.cards .do-it-card {
  grid-area: do-it-card;
  background-color: #edf7ff;
}
.cards .schedule-card {
  grid-area: schedule-card;
  background-color: #f1e9f8;
}
.cards .relegate-card {
  grid-area: relegate-card;
  background-color: #fdedf0;
}
.cards .dont-do-card {
  grid-area: dont-do-card;
  background-color: #fff5f1;
}
.delete-btn, .complete-btn {
  background: coral;
  color: white;
  border: none;
  padding: 15px;
  cursor: pointer;
}
